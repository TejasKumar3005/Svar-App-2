    Label :
        id : label1
        text : "OHH!! PLEASE TRY AGAIN"
        bold : True
        color:(0.9,0,0,1)
        pos_hint: {"center_x":.65, "center_y":.6}
        font_size: 100
        font_name : "BuxtonSketch"

    SmoothButton:
        font_size: 40
        text:"NEXT"
        font_name : "BuxtonSketch"
        bold : True
        back_color: (0,0.9,0,1)
        size_hint: .15,.1
        pos_hint: {"center_x":.75, "center_y":.4}
        on_release:
            root.next()

    SmoothButton:
        font_size: 40
        text:"TRY AGAIN"
        font_name : "BuxtonSketch"
        bold : True
        back_color: (0.9,0,0,1)
        size_hint: .15,.1
        pos_hint: {"center_x":.55, "center_y":.4}
        on_release:
            root.tryagain()

    Image :
        size : 5,5
        background_normal: 'Chemistry2.jpg'
